    "# 나이순 정렬\n",
    "\n",
    "n = int(input())\n",
    "a = [input().split() for i in range(n)]\n",
    "for i in range(len(a)):\n",
    "    a[i][0] = int(a[i][0])\n",
    "    a[i].append(i)\n",
    "a.sort(key=lambda x: (x[0],x[2]) )\n",
    "for i in a:\n",
    "    print(i[0], i[1])"
